<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
	<application>chivalrylobby-web</application>
	<version>1</version>
	<threadsafe>true</threadsafe>

	<system-properties>
		<property name="java.util.logging.config.file" value="WEB-INF/logging.properties" />
	</system-properties>

	<!-- Static files -->
	<static-files>
		<include path="/*.ico" />
		<include path="/robots.txt" />
		<include path="/static/**" />
	</static-files>

	<!-- Page speed optimization -->
	<pagespeed>
		<enabled-rewriter>CollapseWhitespace</enabled-rewriter>
		<enabled-rewriter>RemoveComments</enabled-rewriter>
		<enabled-rewriter>MinifyCss</enabled-rewriter>
	</pagespeed>
</appengine-web-app>
